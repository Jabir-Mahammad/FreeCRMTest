package string;

public class ComparisionDemo {
	public static void main(String[] args) {
		//equals();
		//equalsIgnoreCase();
		contentEquals();
	}
	
	//equals
	static void equals()
	{
		String str1 = "Bangalore";
		String str2 = "Bangalore";
		
		boolean blnRes = str1.equals(str2);
		System.out.println(blnRes);
	}
	
	//equalsIgnoreCase
	static void equalsIgnoreCase()
	{
		String str1 = "Bangalore";
		String str2 = "BaNgAlOrE";
		
		boolean blnRes = str1.equalsIgnoreCase(str2);
		System.out.println(blnRes);
	}
	
	
	//contentEquals
	static void contentEquals()
	{
		String str1 = "Bangalore";
		String str2 = "bangalore";
		
		boolean blnRes = str1.contentEquals(str2);
		System.out.println(blnRes);
	}
}