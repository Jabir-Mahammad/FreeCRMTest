package programs;

public class MissingNumberArray {

	public static void main(String[] args) {
		
		int arr[] = {1,2,3,7,8,10};
		int num=1;
		for(int i=0;i<arr.length;i++) {
		
			if(arr[i]!=num) {
			
				System.out.println("missig number:"+num);
				i--;
			}
			
			
			num++;
		
			
			
		}

	}

}
